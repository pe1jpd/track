prstr:      movem.l d0-d7/a0-a6,-(sp)
            move.l  d0,a0
            lea.l   tft,a3
prstr1      move.b  (a0)+,d0
            beq.s   prstr9
            bsr     prchr
            bra.s   prstr1
prstr9      movem.l (sp)+,d0-d7/a0-a6
            rts

