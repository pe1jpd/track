            psect   tft,0,0,0,0,0,0

tft         equ     $800200         /* 68020 board */


SMALL       equ     0
MEDIUM      equ     1
BIG         equ     2

            vsect
x           dc.w    1
y           dc.w    1
col         dc.w    1
row         dc.w    1
fcolor      dc.w    1
bcolor      dc.w    1
font        dc.l    1
xsize       dc.w    1
ysize       dc.w    1
bytpch      dc.w    1
            ends

* d0: color, d1.l: pix
fastfill    move.l  d1,-(sp)
            lea.l   2(a3),a0
ff1         move    d0,(a0)
            dbra    d1,ff1
            swap    d1                  check if more than 65536
            beq.s   ff3             
            move    #-1,d1
ff2         move    d0,(a0)             
            dbra    d1,ff2
ff3         move.l  (sp)+,d1
            rts

tft_init:   lea.l   tft,a3

            move    #$e8,(a3)           set PLL MN
            move    #$23,2(a3)          N=35
            move    #$02,2(a3)          M=2
            move    #$54,2(a3)          dummy

            move    #$e0,(a3)           start PLL
            move    #$01,2(a3)          enable PLL
            move    #10,d7
            bsr     delay           

            move    #$e0,(a3)           start PLL (2)
            move    #$03,2(a3)          lock PLL
            move    #10,d7
            bsr     delay           

            move    #$01,(a3)           software reset
            move    #100,d7             5ms minimum
            bsr     delay           

            move    #$e6,(a3)           set lshift (pic clk) freq
            move    #$01,2(a3)          $11fff = 73727
            move    #$1f,2(a3)
            move    #$ff,2(a3)

            move    #$b0,(a3)           set LCD mode
            move    #$20,2(a3)          24bit
            move    #$00,2(a3)      
            move    #$01,2(a3)
            move    #$df,2(a3)
            move    #$01,2(a3)
            move    #$0f,2(a3)
            move    #$00,2(a3)

            move    #$b4,(a3)           set horiz. period
            move    #$02,2(a3)
            move    #$13,2(a3)
            move    #$00,2(a3)
            move    #$08,2(a3)
            move    #$2b,2(a3)
            move    #$00,2(a3)
            move    #$02,2(a3)
            move    #$00,2(a3)

            move    #$b6,(a3)           set vertical period
            move    #$01,2(a3)
            move    #$20,2(a3)
            move    #$00,2(a3)
            move    #$04,2(a3)
            move    #$0c,2(a3)
            move    #$00,2(a3)
            move    #$02,2(a3)

            move    #$ba,(a3)           set GPIO value
            move    #$0f,2(a3)

            move    #$b8,(a3)           set GPIO config
            move    #$07,2(a3)
            move    #$01,2(a3)

            move    #$36,(a3)           set address mode
            move    #$22,2(a3)

            move    #$f0,(a3)           set pixel data iface
            move    #$03,2(a3)          16 bit

            move    #1,d7
            bsr     delay

            moveq   #0,d0
            moveq   #0,d1
            move    #479,d2
            move    #271,d3
            bsr     setxy

            move    #$29,(a3)           set display on

            move    #$be,(a3)           set PWM config
            move    #$06,2(a3)
            move    #$f0,2(a3)
            move    #$01,2(a3)
            move    #$f0,2(a3)
            move    #$00,2(a3)
            move    #$00,2(a3)

            move    #$d0,(a3)           set DBC config
            move    #$0d,2(a3)

            move    #$2c,(a3)           write memory start
            rts

setFont:    rts
*           lea.l   bigfont(pc),a0      big font 16x16
*           move.l  a0,font(a6)
*           move    #16,xsize(a6)
*           move    #16,ysize(a6)
*           move    #32,bytpch(a6)
*           rts

setFgColor: move    d0,fcolor(a6)
            rts

setBgColor: move    d0,bcolor(a6)
            rts

* (x1,y1) = d0,d1
* (x2,y2) = d2,d3
setxy:      swap    d6
            lea.l   tft,a3
            move    #$2a,(a3)
            move    d0,d6
            lsr     #8,d6
            move    d6,2(a3)
            move    d0,2(a3)
            move    d2,d6
            lsr     #8,d6
            move    d6,2(a3)
            move    d2,2(a3)
            move    #$2b,(a3)
            move    d1,d6
            lsr     #8,d6
            move    d6,2(a3)
            move    d1,2(a3)
            move    d3,d6
            lsr     #8,d6
            move    d6,2(a3)
            move    d3,2(a3)
            move    #$2c,(a3)
            swap    d6
            rts

* (d0,d1) = (x,y)
putpix:     move    d0,d2
            move    d1,d3
            bsr     setxy
            move    fcolor(a6),2(a3)
            rts

cls:        moveq   #0,d0
            move    d0,x(a6)
            move    d0,y(a6)
            move    d0,col(a6)
            move    d0,row(a6)
            moveq   #0,d0
            moveq   #0,d1
            move    #479,d2
            move    #271,d3
            bsr     setxy
            move    bcolor(a6),d0
            move.l  #(272*480),d1
            bsr     fastfill
            moveq   #BIG,d0
            bsr     setFont
            rts

*move:      move    d0,x(a6)
            move    d1,y(a6)
            rts

*draw:      lea.l   setPixel(pc),a4
            move    x(a6),d4
            move    d0,x(a6)
            move    y(a6),d5
            move    d1,y(a6)
            lea.l   tft,a3
            moveq   #1,d6               xstep=1
            sub     d0,d4               dx=x2-x1
            bpl.s   dr1                 if dx<0
            neg     d4                  dx=-dx
            moveq   #-1,d6              xstep=-1
dr1         moveq   #1,d7               ystep=1
            sub     d1,d5               dy=y2-y1
            bpl.s   dr2                 if dy<0
            neg     d5                  dy=-dy
            moveq   #-1,d7              ystep=-1
dr2         moveq   #0,d2
            move    d4,d3
            cmp     d5,d3               if dx>dy
            bcc.s   dr3                 then draw hor slabs
            move    d5,d3               else
            bra.s   dr4                 draw vert slabs
drx         add     d5,d2
            cmp     d4,d2
            bcs.s   dr6
            sub     d4,d2
            add     d7,d1               y++
dr6         add     d6,d0               x++
dr3         jsr     (a4)                set pixel at (d0,d1)
            dbra    d3,drx
            rts
dry         add     d4,d2
            cmp     d5,d2
            bcs.s   dr7
            sub     d5,d2
            add     d6,d0
dr7         add     d7,d1
dr4         jsr     (a4)
            dbra    d3,dry
            rts
            
setPixel    bsr     setxy
            move    fcolor(a6),2(a3)
            rts

clrPixel    bsr     setxy
            move    bcolor(a6),2(a3)
            rts

copy2scr:   move.l  d0,a1               picture buffer
            lea.l   tft,a3
            lea.l   2(a3),a0            tft dataptr
            moveq   #0,d0               set line xy
            move    #479,d2
            move    #271,d1
cs0         move    d1,d3
            bsr     setxy
            move    #479,d7             copy 1 line
cs1         move    (a1)+,(a0)
            dbra    d7,cs1
            subq    #1,d1
            cmpi    #16,d1
            bpl.s   cs0
            rts

delay       move    #100,d6
del1        dbra    d6,del1
            dbra    d7,delay
            rts

cursor:     move    d0,row(a6)
            move    d1,col(a6)
            rts

prchr       cmpi.b  #$0d,d0
            bne.s   pr1
            clr     col(a6)
            move.b  #$0a,d0
pr1         cmpi.b  #$0a,d0
            bne.s   pr2
            addi    #1,row(a6)
            rts

pr2         lea.l   font10x16(pc),a1
            andi    #$ff,d0
            subi    #$20,d0
*           mulu    #24,d0              bytes/char
            mulu    #20,d0              bytes/char
            adda    d0,a1               offset in stdfont

            move    col(a6),d0          cursor position hor
            mulu    #10,d0              xsize
            addi    #1,col(a6)          increment it for next char
            move    d0,d2           
            add     #9,d2               x2=x1+fontwidth

            move    row(a6),d7
            mulu    #16,d7              ysize
            move    #256,d1
            sub     d7,d1
            move    d1,d3
            add     #15,d3

            bsr     setxy

            move    bcolor(a6),d3
            move    fcolor(a6),d4

            move    #9,d7
lp0         move    (a1)+,d5
            move    #15,d6
lp          lsl     #1,d5
            bcs.s   lpset
            move    d3,2(a3)
            dbra    d6,lp
            dbra    d7,lp0
            rts
lpset       move    d4,2(a3)
            dbra    d6,lp
            dbra    d7,lp0
            rts

